{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
    "jsx": "react-jsx",
		"allowJs": true,
		"checkJs": true,
    "noEmit": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": false,
		"skipLibCheck": true
	},
  "include": ["src/**/*.js", "src/**/*.jsx", "src/types/**/*"],
  "exclude": ["node_modules"]
}
